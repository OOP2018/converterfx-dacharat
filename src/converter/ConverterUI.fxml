<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>

<FlowPane alignment="CENTER" prefHeight="72.0" prefWidth="825.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="converter.ConverterController">
   <children>
      <TextField fx:id="textField1" onAction="#onEnter">
         <FlowPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </FlowPane.margin>
      </TextField>
      <ComboBox fx:id="combo1" prefWidth="150.0">
         <FlowPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </FlowPane.margin>
      </ComboBox>
      <TextField id="textFiled2" fx:id="textField2" onAction="#onEnter">
         <FlowPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </FlowPane.margin>
      </TextField>
      <ComboBox fx:id="combo2" prefWidth="150.0">
         <FlowPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </FlowPane.margin>
      </ComboBox>
      <Button fx:id="convertButton" mnemonicParsing="false" onAction="#handleConvert" text="Convert">
         <FlowPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </FlowPane.margin>
      </Button>
      <Button id="clearButton" fx:id="clearButton" mnemonicParsing="false" onAction="#handleClear" text="Clear">
         <FlowPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </FlowPane.margin>
      </Button>
   </children>
</FlowPane>
